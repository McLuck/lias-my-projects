<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.compsis.sigf.domain.MotivoFechamentoPista" table="motivocorrecao">
    <id column="id" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property name="fimTurno"/>
    <property name="refeicao"/>
    <property name="trocaTurno"/>
    <property name="qtoBanheiro"/>
    <property name="fechamentoAntecipado"/>
    <property name="manutencaoPista"/>
    <property name="ginastica"/>
    <property name="qre"/>
    <property name="servicoInterno"/>
    
  </class>
</hibernate-mapping>
