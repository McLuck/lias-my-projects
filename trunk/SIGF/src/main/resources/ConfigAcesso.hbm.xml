<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.compsis.sigf.domain.ConfigAcesso"  table="config_acesso">
    <id name="id" column="id" type="integer">
        <generator class="increment" />
    </id>
    <property name="complexidade"/>
    <property name="diaSenhaExpirar"/>
    <property name="qtdMinimaSegura"/>
    <property name="senhaExpira"/>
    <property name="statusOperadorInativo"/>
    <property name="qtdRegistroSenha"/>
    <property name="senhaValidade"/>
    <property name="toleranciaBloqueio"/>
    <property name="tempoBloqueio"/>
  </class>
</hibernate-mapping>
