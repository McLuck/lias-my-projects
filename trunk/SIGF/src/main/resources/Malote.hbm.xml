<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.compsis.sigf.domain.Malote" table="malote">
    <id column="id" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property name="detalheFormatoArquivo"/>
    <property name="diasSemRepetirLacre"/>
    <property name="lacreRepete"/>
    <property name="opcCriacaoMalote"/>
    <property name="opcExibicaoInterface"/>
    <property name="opcRegistroDeposito"/>
  </class>
</hibernate-mapping>
