<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.compsis.sigf.domain.ServidorBD" table="servidorbd">
    <id column="id" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property name="bd"/>
    <property name="os"/>
    <property name="ip"/>
    <property name="versao"/>
    <property name="idioma"/>
    <property name="edicao"/>
    <property name="osDetalhe"/>
    <property name="idomaOutro"/>
    <property name="edicaoOutros"/>
    <property name="linuxDetalhe"/>
  </class>
</hibernate-mapping>
