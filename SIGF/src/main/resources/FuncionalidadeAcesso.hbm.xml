<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.compsis.sigf.domain.FuncionalidadeAcesso" table="funcionalidadeacesso">
    <id column="id" name="id">
      <generator class="increment"/>
    </id>
    <property name="acesso"/>
    <property name="acessoMetodo"/>
    <property name="nivelPorMetodo"/>
    <property name="idFuncionalidade" column="idfuncionalidade" type="integer"/>
  </class>
</hibernate-mapping>
