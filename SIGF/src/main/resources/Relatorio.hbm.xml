<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.compsis.sigf.domain.Relatorio" table="relatorio">
    <id column="id" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="arquivo" name="arquivo"/>
    <property column="classificacao" name="classificacao"/>
    <property column="comentario" name="comentario"/>
    <property column="descricao" name="descricao"/>
    <property column="horasD" name="horasDesnvolvimento"/>
    <property column="horasH" name="horasHomologacao"/>
    <property column="idgrupo" name="idGrupo"/>
    <property column="idrelatorio" name="idrelatorio"/>
    <property column="listarelatorio" name="listaRelatorio"/>
    <property column="titulo" name="titulo"/>
    <property column="valorpercebido" name="valorPercebido"/>
    <property column="versao" name="versao"/>
    <property column="arquivoDetalhe" name="arquivoDetalhe"/>
    <property column="usebean" name="usebean"/>
    <property column="relatorioclass" name="relatorioClass"/>
  </class>
</hibernate-mapping>
